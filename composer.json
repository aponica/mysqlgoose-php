    "mariadb",
    "postgresql",
    "sqlserver"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    { "name": "Opplaud LLC", "email": "dev.support@aponica.com" }
  ],
  "require": {
